{
  "name": "regsimple",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "server-dev": "NODE_ENV=development nodemon --exec babel-node ./server/src/index.js",
    "client-dev": "cd client && elm-app start",
    "server-build": "babel ./server/src --out-dir ./server/build",
    "client-build": "npm run css-build && cd client && elm-app build ",
    "css-build": "postcss ./client/src/css/index.css --output ./client/public/style.min.css --config ./postcss.config.js",
    "css-dev": "postcss ./client/src/css/index.css --output ./client/public/style.min.css --watch --config ./postcss.config.js",
    "lint": "prettier --write './server/**/*.js'",
    "server-test": "NODE_ENV=test jest",
    "client-test": "cd client && elm-app test",
    "test": "npm run client-test && npm run server-test"
  },
  "author": "",
  "pre-commit": [
    "test",
    "lint"
  ],
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "passwordless": "^1.1.2",
    "passwordless-postgrestore": "github:Battochon/passwordless-postgrestore",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "env2": "^2.2.0",
    "jest": "^22.4.4",
    "nodemon": "^1.17.4",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^5.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-import": "^11.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "1.12.1",
    "supertest": "^3.1.0",
    "tachyons-custom": "^4.9.4"
  }
}
